{
  "manifest_version": 2,
  "name": "LDP Form Filler",
  "version": "1.0.0",
  "description": "Fill in test data",
  "content_scripts": [
    {
      "matches": ["http://mfind.apartmentlist.com/ldp*"],
      "js": [ "jquery-2.0.3.js", "ldp.js" ],
      "run_at": "document_idle"
    }
  ],
  "page_action": {
    "default_icon": {
      "38": "listy38.png"
    },
    "default_title": "LDP Form Filler"
  },
  "web_accessible_resources": [
    "ldp.js",
    "background.js",
    "jquery-2.0.3.js"
  ],
  "background": {
    "scripts": [ "background.js" ]
  }
}
